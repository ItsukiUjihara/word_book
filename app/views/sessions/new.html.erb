<h2>userログイン</h2>
<%= form_with url:create_sessions_path,local: true,method: :post do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :email %> 
  <%= form.email_field :email %>
  <%= form.label :password %>
  <%= form.password_field :password %>
  <%= form.label :password_confirmation %>
  <%= form.password_field :password_confirmation %>
  <%= form.submit 'ログイン' %>
<% end %>